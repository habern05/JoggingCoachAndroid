<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="at.haraldbernhard.joggingcoachandroid">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LaunchActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CreateUserActivity"
            android:label="@string/title_activity_create_user"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".IntervalDialogActivity"
            android:label="@string/title_activity_interval_dialog"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".IntervalModeActivity" />
        <activity android:name=".MenuActivity"></activity>
    </application>

</manifest>